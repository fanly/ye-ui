{
  "name": "@ye-ui/components",
  "type": "module",
  "version": "0.0.3-alpha.4",
  "private": false,
  "description": "一个简单的组件库",
  "author": "fanly <https://github.com/fanly>",
  "license": "MIT",
  "homepage": "https://github.com/fanly/ye-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/fanly/ye-ui.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/fanly/ye-ui/issues"
  },
  "keywords": [
    "ui",
    "ye",
    "vue3",
    "unocss",
    "component",
    "component library"
  ],
  "exports": {
    ".": {
      "import": "./dist/components.js",
      "require": "./dist/components.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/components.umd.cjs",
  "module": "./dist/components.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunx --bun vite -d",
    "build": "vue-tsc -b && bun run vite build",
    "preview": "bunx --bun vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "format": "bun prettier . --write"
  },
  "dependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@unocss/preset-web-fonts": "^0.61.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "3.3.2",
    "typescript": "^5.2.2",
    "unocss": "^0.61.0",
    "vite": "^5.3.1",
    "vite-plugin-linter": "^2.1.1",
    "vue-tsc": "^2.0.21"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
