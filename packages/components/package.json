{
  "name": "@ye-ui/components",
  "type": "module",
  "version": "0.0.3-alpha.5",
  "private": false,
  "description": "一个简单的组件库",
  "author": "fanly <https://github.com/fanly>",
  "license": "MIT",
  "homepage": "https://github.com/fanly/ye-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/fanly/ye-ui.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/fanly/ye-ui/issues"
  },
  "keywords": [
    "ui",
    "ye",
    "vue3",
    "unocss",
    "component",
    "component library"
  ],
  "exports": {
    ".": {
      "import": "./dist/components.js",
      "require": "./dist/components.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/components.umd.cjs",
  "module": "./dist/components.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunx --bun vite -d",
    "build": "bun run vite build",
    "preview": "bunx --bun vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "bun prettier . --write"
  },
  "dependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@unocss/eslint-plugin": "^0.61.5",
    "@unocss/preset-web-fonts": "^0.61.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue-vine/eslint-parser": "^0.1.14",
    "eslint": "9.x",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "prettier": "3.3.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.16.1",
    "unocss": "^0.61.0",
    "vite": "^5.3.1",
    "vite-plugin-linter": "^2.1.1",
    "vue-vine": "^0.1.14",
    "vue-vine-tsc": "^0.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
