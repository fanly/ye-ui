{
  "name": "@ye-ui/components",
  "type": "module",
  "version": "0.0.3-alpha.7",
  "private": false,
  "description": "一个简单的组件库",
  "author": "fanly <https://github.com/fanly>",
  "license": "MIT",
  "homepage": "https://github.com/fanly/ye-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/fanly/ye-ui.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/fanly/ye-ui/issues"
  },
  "keywords": [
    "ui",
    "ye",
    "vue3",
    "unocss",
    "component",
    "component library"
  ],
  "exports": {
    ".": {
      "import": "./dist/components.js",
      "require": "./dist/components.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/components.umd.cjs",
  "module": "./dist/components.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunx --bun vite -d",
    "build": "bunx vite build",
    "preview": "bunx --bun vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "bunx --bun prettier . --write"
  },
  "dependencies": {
    "lightningcss": "^1.25.1",
    "vue": "^3.4.34"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@unocss/eslint-plugin": "^0.61.5",
    "@unocss/preset-web-fonts": "^0.61.5",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vue-vine/eslint-parser": "^0.1.14",
    "eslint": "^9.7.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "prettier": "3.3.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "unocss": "^0.61.5",
    "vite": "^5.3.4",
    "vite-plugin-linter": "^2.1.1",
    "vue-vine": "^0.1.14",
    "vue-vine-tsc": "^0.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
